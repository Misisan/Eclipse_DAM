package inicio;

public class SumResta {
			 
	static int suma = 0;
	static void anadir() {
		suma++;
		System.out.println("SUMA: " + suma);
	}
}
 
class Hilos extends Thread {
 
	@Override
	public void run() {
		algo.anadir();
	}
}
			 
public class SumResta {
			 
	public static void main(String[] args) {
		try {
			for (int i = 0; i < 10; i++) {
				Hilos h = new Hilos();
				Hilos h1 = new Hilos();
				h.start();
				h1.start();
			}
		} catch (Exception e) {}
	}
}

