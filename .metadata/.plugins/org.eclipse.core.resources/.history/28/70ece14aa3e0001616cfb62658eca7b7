package inicio;

import java.sql.Date;

public class Pedido {

	private Integer id;
	private Date fecha;
	private Item item;
	
	public Pedido(Date fecha) {
		this.fecha=fecha;
		item = new Item();
	}
	
	public Pedido(){}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Date getFecha() {
		return fecha;
	}

	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}
	
	public void setItem(Item item){
		this.item=item;
	}
	
	public Item getItem(){
		return item;
	}

}
